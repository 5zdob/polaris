{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", "build/**", ".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "test": {},
    "lint": {
      "outputs": [".eslintcache"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "preversion": {
      "cache": false
    }
  }
}
